{
    "key": "",
    "name": "Meet Scribble",
    "description": "Scribble around on your Google Meet video calls",
    "homepage_url": "https://meet.google.com/",
    "version": "0.0.2",
    "browser_action": {
        "default_icon": "icons/icon128.png",
        "default_title": "Meet Scribble",
        "default_popup": "popup/index.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "web_accessible_resources": [
        "build/content.bundle.js",
        "images/*.png",
        "icons/icon.png"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://meet.google.com/*",
                "https://meet-preprod.corp.google.com/*",
                "https://preprod.meet.sandbox.google.com/*"
            ],
            "js": [
                "build/inject.bundle.js"
            ],
            "css": [
                "static/style.css"
            ],
            "run_at": "document_idle",
            "all_frames": false
        }
    ],
    "manifest_version": 2
}